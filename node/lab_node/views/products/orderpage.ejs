        $("#calc2").append("<input type='text' id='C"+this.name+"' name='"+this.name+"' value='1'/>");
        $("#calc2").append("<button class='plus' name='"+this.name+"'>+</button>");
        $("#calc2").append("<button class='mins' name='"+this.name+"'>-</button>");
        $("#calc2").append(" EGP ");
